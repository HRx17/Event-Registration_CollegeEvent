</LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="7dp"
            android:src="@drawable/updatesbanner"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="10dp"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="10dp"
                android:id="@+id/mainlayout"
                android:src="@drawable/ic_baseline_first_page_24"/>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:id="@+id/timetable"
                android:layout_marginLeft="12dp"
                android:src="@drawable/ic_baseline_calendar_24"/>
            <ImageView
                android:layout_width="35dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:src="@drawable/sponsorstransparent1"/>
            <ImageView
                android:layout_width="35dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="6dp"
                android:src="@drawable/sponsorstransparent2"/>
            <ImageView
                android:layout_width="30dp"
                android:layout_marginLeft="6dp"
                android:layout_height="match_parent"
                android:src="@drawable/sponsorstransparent3"/>
            <ImageView
                android:layout_width="35dp"
                android:layout_marginLeft="6dp"
                android:layout_height="match_parent"
                android:src="@drawable/sponsorstransparent4"/>
            <ImageView
                android:layout_width="35dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="6dp"
                android:src="@drawable/sponsorstransparent5"/>
            <ImageView
                android:id="@+id/toinsta"
                android:layout_width="27dp"
                android:layout_marginLeft="25dp"
                android:layout_height="30dp"
                android:layout_gravity="right"
                android:layout_marginRight="20dp"
                android:paddingVertical="0dp"
                android:src="@drawable/ic_insta1jpg" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginTop="4dp"
            android:background="#69FDFDFD"/>
    </LinearLayout>

<ScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/event1"
            android:layout_margin="15dp"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:src="@drawable/abhivyakti" />
        <ImageView
            android:id="@+id/event2"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_margin="27dp"
            android:src="@drawable/avishkar"/>

        <ImageView
            android:id="@+id/event3"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_above="@+id/event5"
            android:layout_margin="27dp"
            android:src="@drawable/bidwarsban" />

        <ImageView
            android:id="@+id/event4"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_above="@+id/event7"
            android:layout_margin="27dp"
            android:src="@drawable/codefiestaban" />
        <ImageView
            android:id="@+id/event5"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_above="@+id/event7"
            android:layout_margin="27dp"
            android:src="@drawable/blindcode" />

        <ImageView
            android:id="@+id/event6"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_alignParentBottom="true"
            android:layout_margin="27dp"
            android:src="@drawable/codestormban" />
        <ImageView
            android:id="@+id/event8"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_alignParentBottom="true"
            android:layout_margin="27dp"
            android:src="@drawable/niyuktiban" />
        <ImageView
            android:id="@+id/event9"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_alignParentBottom="true"
            android:layout_margin="27dp"
            android:src="@drawable/pridictaholic" />
        <ImageView
            android:id="@+id/event10"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_alignParentBottom="true"
            android:layout_margin="27dp"
            android:src="@drawable/scavengerban" />
        <ImageView
            android:id="@+id/event11"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_alignParentBottom="true"
            android:layout_margin="27dp"
            android:src="@drawable/watchcraftban" />
        <ImageView
            android:id="@+id/event12"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:layout_alignParentBottom="true"
            android:layout_margin="27dp"
            android:src="@drawable/enigma" />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#4FFFFFFF"
            android:text="Logout"
            android:id="@+id/logout"
            android:textColor="@color/black"
            android:layout_gravity="center"/>
    </LinearLayout>
</ScrollView>
</LinearLayout>
</LinearLayout>